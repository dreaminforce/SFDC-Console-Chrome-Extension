{
  "name": "SFDC Console",
  "version": "1.1",
  "description": "Opens Developer Console or Execute Anonymous window (if console is already open)",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://*.force.com/*",
    "https://*.salesforce.com/*"
  ],
  "manifest_version": 3
}
